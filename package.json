{
  "name": "ng-hello-world",
  "version": "1.0.0-alpha.0",
  "description": "ng-hello-world",
  "keywords": "Angular hello world",
  "author": "Marc Laval",
  "scripts": {
    "clean": "rm -rf dist/",
    "public": "mkdir dist && cp -a public/*.png dist/",
    "build-en": "webpack --config webpack.config.js --progress --profile --bail",
    "build-fr": "webpack --config webpack.config.js --progress --profile --bail --env.lang=fr",
    "build": "npm run clean && npm run public &&  npm run build-en && npm run build-fr",
    "i18n": "ng-xi18n",
    "start": "http-server ./dist -s"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mlaval/ng-hello-world.git"
  },
  "license": {
    "type": "Apache",
    "url": "https://raw.githubusercontent.com/mlaval/ng-hello-world/master/LICENSE"
  },
  "bugs": {
    "url": "https://github.com/mlaval/optimize-angular-app/issues"
  },
  "homepage": "https://github.com/mlaval/optimize-angular-app#readme",
  "dependencies": {
    "@angular/core": "2.3.1",
    "@angular/compiler": "2.3.1",
    "@angular/common": "2.3.1",
    "@angular/forms": "2.3.1",
    "@angular/http": "2.3.1",
    "@angular/platform-browser": "2.3.1",
    "@angular/platform-browser-dynamic": "2.3.1",
    "@angular/router": "3.3.1",
    "classlist-polyfill": "1.0.3",
    "core-js": "2.4.1",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.3.1",
    "@types/node": "^6.0.38",
    "http-server": "^0.9.0",
    "typescript": "2.0.10",
    "webpack": "2.1.0-beta.27",
    "html-webpack-plugin": "2.24.1",
    "raw-loader": "0.5.1",
    "@ngtools/webpack": "mlaval/angular-cli#ngtools-webpack-snapshot"
  }
}